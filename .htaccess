rewriteEngine on
rewriteCond %{SCRIPT_FILENAME} !-f
rewriteCond %{SCRIPT_FILENAME} !-d
Options -Indexes

#Cadastro de rotas
rewriteRule ^rotas-de-acesso ./start.php [NC]
# Página de login
rewriteRule ^login$ ./start.php [NC]
# Login
rewriteRule ^do-login$ ./start.php [NC]
# Dashboard
rewriteRule ^inicio$ ./start.php [NC]
rewriteRule ^$ ./start.php [NC]

